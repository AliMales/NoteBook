{
  "scripts": {
    "dev": "vitepress",
    "build": "vitepress build",
    "serve": "vitepress serve",
    "preinstall": "npx only-allow pnpm",
    "log": "conventional-changelog -p vue -i CHANGELOG.md -s -r 0",
    "cz": "git add . && git cz && git push",
    "release": "npm run build && gh-pages -d .vitepress/dist"
  },
  "dependencies": {
    "@docsearch/css": "3.0.0-alpha.50",
    "@docsearch/js": "3.0.0-alpha.50",
    "@vue/repl": "^1.0.0",
    "@vueuse/core": "^7.6.2",
    "animate.css": "^4.1.1",
    "axios": "^0.26.0",
    "body-scroll-lock": "4.0.0-beta.0",
    "conventional-changelog": "^3.1.25",
    "dynamics.js": "^1.1.5",
    "gsap": "^3.9.0",
    "husky": "^7.0.4",
    "localStorage": "^1.0.4",
    "localforage": "^1.10.0",
    "normalize.css": "^8.0.1",
    "shiki": "^0.10.1",
    "three": "^0.138.0",
    "vitepress": "^0.22.2",
    "vue": "^3.2.26"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^16.9.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "gh-pages": "^3.2.3",
    "release-it": "^14.12.5"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "react",
        "react-dom",
        "@types/react"
      ]
    }
  }
}
